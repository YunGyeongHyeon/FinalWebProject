<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">
	
		<insert id="insert"	parameterType="com.fireBusters.web.dto.UserMember"><!--dto를 sql로 만드는것 -->
		<selectKey keyProperty="bno" resultType="int" order="BEFORE"><!--order="BEFORE"는 밑에를 실행하기전에 먼저 실행해라  -->
			select REPORT_REPORT_NO_seq.nextval from dual;
		</selectKey>
		insert into board(REPORT_LAT, REPORT_LON)
		values(#{REPORT_LAT}, #{REPORT_LON})<!--get를 호출해서 값을 제공하는것 -->
	</insert>

</mapper>