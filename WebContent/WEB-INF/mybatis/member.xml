<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="adminMember">
	<resultMap id="adminMember" type="com.fireBusters.web.dto.AdminMember">
		<result column="adminid" property="adminId"/>
		<result column="adminpassword" property="adminPassword"/>
	</resultMap>

	<select id="selectAdminMember" parameterType="string" resultMap="adminMember">
 		 select adminid, adminpassword from testadmin where adminid=#{adminid}
	</select>

	
	<select id="selectReport" parameterType="string" resultType="com.fireBusters.web.dto.AdminBoard">
			select report_date, report_lat, report_lon from report where fire_station_no=
			(select fire_station_no from fire_station where fire_station_name=#{name})
	</select>
	
	
	
</mapper>